<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="app.inventario.view.TelaMenuPrincipal">
  <grid id="root" binding="mainPanel" layout-manager="GridLayoutManager" row-count="3" column-count="1" same-size-horizontally="false" same-size-vertically="false">
    <margin top="20" left="20" bottom="20" right="20"/>
    <constraints>
      <xy x="20" y="20" width="300" height="200"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>

      <!-- Linha 1: Título -->
      <component id="1" class="javax.swing.JLabel" binding="txtTitulo">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="0"/>
        </constraints>
        <properties>
          <text value="Catacombs Store"/>
          <horizontalAlignment value="0"/>
        </properties>
      </component>

      <!-- Linha 2: Botão Jogar -->
      <component id="2" class="javax.swing.JButton" binding="btnJogar">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1"/>
        </constraints>
        <properties>
          <text value="Jogar"/>
        </properties>
      </component>

      <!-- Linha 3: Botão Sair -->
      <component id="3" class="javax.swing.JButton" binding="btnSair">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1"/>
        </constraints>
        <properties>
          <text value="Sair"/>
        </properties>
      </component>

    </children>
  </grid>
</form>

